<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="sharedpref" path="." />
        <exclude domain="sharedpref" path="firebase_auth_tokens.xml" />
        <exclude domain="database" path="firebase_cache" />
    </cloud-backup>
    
    <device-transfer>
        <include domain="sharedpref" path="." />
        <exclude domain="sharedpref" path="firebase_auth_tokens.xml" />
    </device-transfer>
</data-extraction-rules>